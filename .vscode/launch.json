{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug cpp",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/sandbox/build/out",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/sandbox/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "/usr/bin/gdb",
      "logging": {
        "moduleLoad": true
      }
    }
  ]
}
